#!/bin/bash -xe

cd ${TEST_TMPDIR}

PATH=${abs_builddir}/../models:${abs_builddir}/../frontend:$PATH
OUTPUT=libnet.a
INPUT=$DATADIR/libnet.a

cp --no-preserve=mode,ownership ${INPUT} ${OUTPUT}.gold
${TOOLCHAIN_PATH}/ranlib ${OUTPUT}.gold

cp --no-preserve=mode,ownership ${INPUT} ${OUTPUT}
